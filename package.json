{
  "name": "surveygizmo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "supervisor node ./bin/www | npm run watch",
    "build": "browserify -t bubleify ./src/app.js  > ./public/javascripts/bundle.js",
    "build-dev": "browserify -d -t bubleify ./src/app.js > ./public/javascripts/bundle.js",
    "watch": "watchify -d -t bubleify ./src/app.js -o ./public/javascripts/bundle.js -v"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "express-compile-sass": "^3.0.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "request": "^2.79.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "bubleify": "^0.7.0",
    "react-dom": "^15.4.2",
    "supervisor": "^0.12.0",
    "watchify": "^3.9.0"
  }
}
